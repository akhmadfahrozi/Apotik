<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.e.apotik">

   <uses-permission android:name="android.permission.INTERNET"/>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".obat_adapter"></activity>
        <activity android:name=".transaksi_adapter" />
        <activity
            android:name=".transaksi_isi"
            android:configChanges="orientation"
            android:screenOrientation="landscape"
            android:parentActivityName=".MainActivity" />

        <meta-data
            android:name=".android.support.PARRENT_ACTIVITY"
            android:value=".MainActivity" />
        <activity android:name=".outlet_adapter" />
        <activity
            android:name=".outlet_isi"
            android:theme="@style/Theme.AppCompat.Light.NoActionBar" />
        <activity android:name=".Histori"/>
        <activity
            android:name=".Outlet"/>
        <activity
            android:name=".Obat"/>

        <activity
            android:name=".MainActivity"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>